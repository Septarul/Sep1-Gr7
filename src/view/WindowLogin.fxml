<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="273.0" prefWidth="441.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" prefHeight="53.0" prefWidth="416.0" text="LOGIN">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <HBox>
         <children>
            <Label prefHeight="51.0" prefWidth="117.0" text="Username">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="usernameField" onKeyTyped="#keyTyped">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0">
         <children>
            <Label prefHeight="51.0" prefWidth="115.0" text="Password">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordField" onKeyTyped="#keyTyped">
               <font>
                  <Font size="24.0" />
               </font>
            </PasswordField>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="errorLabel" textFill="RED">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Pane>
         <children>
            <Button layoutX="117.0" mnemonicParsing="false" onAction="#loginButtonPressed" text="Login">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button layoutX="245.0" mnemonicParsing="false" onAction="#cancelButtonPressed" text="Cancel">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</VBox>
